<AnchorPane id="AnchorPane" prefHeight="440.0" prefWidth="1060.0" style="-fx-border-color: #0066ff; -fx-border-width: 2px;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="list.room.RoomController">
